<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/profile"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileActivity">

    <TextView
        android:id="@+id/myEmail"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="62dp"
        android:layout_marginTop="223dp"
        android:layout_marginEnd="62dp"
        android:layout_marginBottom="348dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/myBack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/name_string"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="13dp"
        android:text="이름"
        app:layout_constraintBottom_toTopOf="@+id/myName"
        app:layout_constraintStart_toStartOf="@+id/myName"
        app:layout_constraintTop_toBottomOf="@+id/myEmail" />

    <TextView
        android:id="@+id/myName"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="62dp"
        android:layout_marginEnd="62dp"
        android:layout_marginBottom="257dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/myPasswordChange"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name_string" />

    <TextView
        android:id="@+id/email_string"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="189dp"
        android:layout_marginBottom="13dp"
        android:text="이메일"
        app:layout_constraintBottom_toTopOf="@+id/myEmail"
        app:layout_constraintStart_toStartOf="@+id/myEmail"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/myPasswordChange"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="62dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="76dp"
        android:text="비밀번호 변경"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/myBack"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myName" />

    <Button
        android:id="@+id/myBack"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="62dp"
        android:layout_marginBottom="76dp"
        android:text="뒤로가기"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/myPasswordChange"
        app:layout_constraintTop_toBottomOf="@+id/myEmail" />


</androidx.constraintlayout.widget.ConstraintLayout>